{
    "name": "@magda/builder-docker",
    "version": "0.0.38-0",
    "description": "Builder image for nodejs magda projects",
    "scripts": {
        "docker-build-prod":
            "create-docker-context-for-node-component --build --push --tag latest",
        "docker-build-local":
            "create-docker-context-for-node-component --build --push --tag latest --local"
    },
    "author": "",
    "license": "Apache-2.0",
    "config": {
        "docker": {
            "name": "data61/magda-builder-docker",
            "include":
                "Dockerfile modprobe.sh docker-entrypoint.sh release-architectures"
        }
    },
    "devDependencies": {
        "@magda/scripts": "^0.0.38-0",
        "@magda/builder-nodejs": "^0.0.38-0"
    },
    "magda": {
        "categories": {
            "builder": true
        }
    }
}
