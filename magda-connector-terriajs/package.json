{
  "name": "@magda/connector-terriajs",
  "description": "MAGDA TerriaJS Connector",
  "version": "0.0.32-0",
  "scripts": {
    "build": "npm run compile",
    "compile": "npm run compile-node && npm run compile-browser",
    "compile-node": "tsc -p tsconfig-build.json",
    "compile-browser": "webpack --config src/webpack.config.js",
    "watch": "tsc -p tsconfig-build.json --watch",
    "start": "node dist/index.js",
    "dev": "run-typescript-in-nodemon src/index.ts",
    "docker-build-local": "create-docker-context-for-node-component --build --push --tag auto --local",
    "docker-build-prod": "create-docker-context-for-node-component --build --push --tag auto"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@magda/scripts": "^0.0.32-0",
    "@types/express": "^4.0.37",
    "@types/node": "^8.0.14",
    "@types/request": "^2.0.0",
    "@types/urijs": "^1.15.34",
    "@types/xmldom": "^0.1.29",
    "@types/yargs": "^6.6.0",
    "isomorphic-fetch": "^2.2.1",
    "ts-loader": "^2.3.7",
    "typescript": "~2.5.0",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@magda/ckan-connector": "^0.0.32-0",
    "@magda/registry-aspects": "^0.0.32-0",
    "@magda/typescript-common": "^0.0.32-0",
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "moment": "^2.17.1",
    "request": "^2.79.0",
    "terriajs": "^5.5.4",
    "terriajs-cesium": "^1.33.3",
    "urijs": "^1.18.12",
    "xmldom": "^0.1.27",
    "xmlhttprequest": "TerriaJS/node-XMLHttpRequest#1.8.0-terriajs",
    "yargs": "^8.0.1"
  },
  "config": {
    "registryUrl": "http://localhost:6101/v0",
    "docker": {
      "name": "data61/magda-connector-terriajs",
      "include": "node_modules aspect-templates dist Dockerfile"
    },
    "jwtSecret": "squirrel",
    "userId": "00000000-0000-4000-8000-000000000000"
  }
}
