{
    "name": "@magda/migrator-combined-db",
    "version": "0.0.33-0",
    "description":
        "A container that combined all MAGDA's Postgres databases, for use locally or in resource-constrained environments",
    "scripts": {
        "dev":
            "create-pod-and-forward --configuration ../deploy/kubernetes/local/base/combined-db.yml --selector service=combined-db --port 5432",
        "docker-build-local":
            "create-docker-context-for-node-component --build --push --tag auto --local",
        "docker-build-prod":
            "create-docker-context-for-node-component --build --push --tag auto"
    },
    "author": "",
    "license": "Apache-2.0",
    "config": {
        "docker": {
            "name": "data61/magda-migrator-combined-db",
            "include":
                "Dockerfile node_modules/@magda/authorization-db/sql node_modules/@magda/registry-datastore/sql node_modules/@magda/session-store/sql node_modules/@magda/discussions-db/sql"
        }
    },
    "devDependencies": {
        "@magda/migrator-authorization-db": "^0.0.33-0",
        "@magda/migrator-discussions-db": "^0.0.33-0",
        "@magda/migrator-registry-db": "^0.0.33-0",
        "@magda/migrator-session-db": "^0.0.33-0",
        "@magda/scripts": "^0.0.33-0"
    }
}
