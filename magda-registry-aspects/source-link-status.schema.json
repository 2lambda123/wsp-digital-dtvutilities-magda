{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "Aspect describing source link status of a distribution (active or broken)",
    "type": "object",
    "properties": {
        "status": {
            "enum": ["active", "broken"]
        },
        "httpStatusCode": {
            "type": "integer"
        },
        "errorDetails": {
            "type": "string"
        }
    }
}
