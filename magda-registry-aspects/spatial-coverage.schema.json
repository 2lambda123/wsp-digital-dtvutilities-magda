{
    "$schema": "http://json-schema.org/schema#",
    "title": "Spatial extent defined (will try to confom as much to) in geojson.",
    "type": "object",
    "properties": {
        "bbox": {
            "title": "Bounding box in order minlon (west), minlat (south), maxlon (east), maxlat (north)",
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "spatialDataInputMethod": {
            "title": "the spatial coverage has been specified by which type of data. Map reserves for future use only",
            "type": "string",
            "enum": ["bbox", "region", "map"]
        },
        "uiHierarchy": {
            "type": "object",
            "properties": {
                "steId": {
                    "title": "STE region Id",
                    "type": "string"
                },
                "sa4Id": {
                    "title": "SA4 region Id",
                    "type": "string"
                },
                "sa3Id": {
                    "title": "SA4 region Id",
                    "type": "string"
                }
            }
        }
    }
}
